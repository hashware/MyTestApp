<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tao.jiang.mytestapp">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.DownloadActivity" />
        <activity android:name=".ui.ServiceActivity" />

        <service android:name=".MyService" />

        <activity android:name=".ui.CustomViewActivity" />
        <activity android:name=".ui.HorizontalScrollViewExActivity" />
        <activity android:name=".ui.ListView1Activity" />
        <activity android:name=".ui.PopupActivity"></activity>
    </application>

</manifest>